<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.gabriel.equalscase.mapper.mastervisa.TrailerMapperMasterVisa">

    <insert id="insert" parameterType="com.gabriel.equalscase.model.visamaster.TrailerMasterVisa"
            useGeneratedKeys="true" keyProperty="id">
        INSERT INTO trailer (
            tipo_registro, total_registro, reservado
        ) VALUES (
            #{tipoRegistro}, #{totalRegistro}, #{reservado}
        )
    </insert>

    <select id="findFirst" resultType="com.gabriel.equalscase.model.visamaster.TrailerMasterVisa">
        SELECT * FROM trailer LIMIT 1
    </select>

</mapper>
