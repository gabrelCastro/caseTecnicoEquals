<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.gabriel.equalscase.mapper.TrailerMapper">

    <insert id="insert" parameterType="Trailer" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO trailer (
            tipo_registro, total_registro, reservado
        ) VALUES (
            #{tipoRegistro}, #{totalRegistro}, #{reservado}
        )
    </insert>

</mapper>
